@page
@model _17bang.Pages.Log.OnModel
@{
    ViewData["Title"] = "On";
}

<form method="post">
    <div>
        <label>用户名：（* 必填）</label>
        <input asp-for="@Model.LogOnModel.Name" />
        <span asp-validation-for="@Model.LogOnModel.Name"></span>
    </div>
    <div>
        <label>密码：（* 必填）</label>
        <input asp-for="@Model.LogOnModel.Password" />
        <span asp-validation-for="@Model.LogOnModel.Password"></span>
    </div>
    <div>
        <label>验证码：（* 必填） </label>
        <input type="text" /> <img src="~/Image/一起帮logo.jpg" asp-append-version="true" />
    </div>
    <button type="submit">提交</button>
    <input asp-for="@Model.RememberMe" />记住我
    <a asp-page="/Password/Forget">忘记密码?</a>
    <a asp-page="/Register/Register">注册</a>
</form>

<h1>session:@Model.ViewData["name"]</h1>
